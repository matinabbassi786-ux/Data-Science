import requests


for x in range(1,51):
    url = f"https://books.toscrape.com/catalogue/page-{x}.html"
    response = requests.get(url)
    with open(f"HTML/page-{x}.html","w",encoding="utf-8") as f:
        f.write(response.text)
    print(f'Pag Done{x} ')

